CREATE TABLE `email_addresses` (
    `id` BIGINT UNSIGNED NOT NULL,
    `alias` BIGINT UNSIGNED NOT NULL COMMENT 'foreign to email_aliasses.id',
    `domain` BIGINT UNSIGNED NOT NULL COMMENT 'foreign to domains.id',
     UNIQUE KEY `id` (`id`),
     KEY `index` (`id`)
)  ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;
